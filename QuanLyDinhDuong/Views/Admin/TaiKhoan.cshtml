                                    <th>
                                        Mã chức vụ
                                    </th>
                                    <th>
                                        Chức năng
                                    </th>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model)
                                        {
                                        <tr>
                                            <td>
                                                @item.IDTAIKHOAN
                                            </td>
                                            <td>
                                                @item.HOTEN
                                            </td>
                                            <td>
                                                @item.SDT
                                            </td>
                                            <td>
                                                @item.GIOITINH
                                            </td>
                                            <td class="text-primary">
                                                @item.MACHUCVU
                                            </td>
                                            <td class="text-primary">
                                                <a href="#" class="fa fa-remove" style="margin-left:15px; margin-right:20px;"></a>
                                                <a href="#" class="fa fa-edit"></a>
                                            </td>
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>


